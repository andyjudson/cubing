{
  "name": "random-uint-below",
  "version": "2.0.0",
  "description": "A library for generating random integers.",
  "author": "Lucas Garron <code@garron.net> (https://garron.net/)",
  "license": "(MIT OR GPL-3.0-or-later)",
  "repository": "github:lgarron/random-int-js",
  "bugs": {
    "url": "https://github.com/lgarron/random-int-js/issues"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./global": {
      "import": "./dist/esm-global/index.js",
      "types": "./dist/types/global.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build-types && npm run build-js",
    "build-types": "npx tsc",
    "build-js": "node script/build.js",
    "dev": "node script/dev.js",
    "clean": "rm -rf ./dist",
    "prepack": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=19"
  },
  "devDependencies": {
    "barely-a-dev-server": "^0.3.6",
    "typescript": "^4.9.3"
  },
  "files": [
    "/LICENSE.md",
    "/README.md",
    "/package.json",
    "/package-lock.json",
    "/dist/**/*"
  ]
}
